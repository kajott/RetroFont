defmap "mc6847sg4"
    map 4.00  U+00A0  # NO-BREAK SPACE
    map 4.01  U+2598  # QUADRANT UPPER LEFT
    map 4.02  U+259D  # QUADRANT UPPER RIGHT
    map 4.03  U+2580  # UPPER HALF BLOCK
    map 4.04  U+2596  # QUADRANT LOWER LEFT
    map 4.05  U+258C  # LEFT HALF BLOCK
    map 4.06  U+259E  # QUADRANT UPPER RIGHT AND LOWER LEFT
    map 4.07  U+259B  # QUADRANT UPPER LEFT AND UPPER RIGHT AND LOWER LEFT
    map 4.08  U+2597  # QUADRANT LOWER RIGHT
    map 4.09  U+259A  # QUADRANT UPPER LEFT AND LOWER RIGHT
    map 4.10  U+2590  # RIGHT HALF BLOCK
    map 4.11  U+259C  # QUADRANT UPPER LEFT AND UPPER RIGHT AND LOWER RIGHT
    map 4.12  U+2584  # LOWER HALF BLOCK
    map 4.13  U+2599  # QUADRANT UPPER LEFT AND LOWER LEFT AND LOWER RIGHT
    map 4.14  U+259F  # QUADRANT UPPER RIGHT AND LOWER LEFT AND LOWER RIGHT
    map 4.15  U+2588  # FULL BLOCK

font "MT0A" "Motorola MC6847 (ASCII only)"
    usemap "mc6847base"

font "MT04" "Motorola MC6847 (with 2x2 semigraphics)"
    usemap "mc6847base"
    usemap "mc6847sg4"

font "MT06" "Motorola MC6847 (with 2x3 semigraphics)"
    usemap "mc6847base"
    usemap "mc6847sg6"

font "MT0Z" "Motorola MC6847 (with all semigraphics)"
    usemap "mc6847base"
    usemap "mc6847sg4"
    usemap "mc6847sg6"

font "MT14" "Motorola MC6847T1"
    usemap "mc6847t1"
    usemap "mc6847sg4"

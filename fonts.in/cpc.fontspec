    map 7.13  U+266A  # EIGHTH NOTE
    map 7.14  U+263C  # WHITE SUN WITH RAYS
    map 7.15  U+FFBDB # rocket
    map 7.16  U+2B61  # UPWARDS TRIANGLE-HEADED ARROW
    map 7.17  U+2B63  # DOWNWARDS TRIANGLE-HEADED ARROW
    map 7.18  U+2B60  # LEFTWARDS TRIANGLE-HEADED ARROW
    map 7.19  U+2B62  # RIGHTWARDS TRIANGLE-HEADED ARROW
    map 7.20  U+25B2  # BLACK UP-POINTING TRIANGLE
    map 7.21  U+25BC  # BLACK DOWN-POINTING TRIANGLE
    map 7.22  U+25B6  # BLACK RIGHT-POINTING TRIANGLE
    map 7.23  U+25C0  # BLACK LEFT-POINTING TRIANGLE
    map 7.24  U+1FBC6 # STICK FIGURE WITH ARMS RAISED
    map 7.25  U+1FBC5 # STICK FIGURE
    map 7.26  U+1FBC7 # STICK FIGURE LEANING LEFT
    map 7.27  U+1FBC8 # STICK FIGURE LEANING RIGHT
    map 7.28  U+FFBDC # bomb
    map 7.29  U+FFBDD # UFO
    map 7.30  U+2195  # UP DOWN ARROW
    map 7.31  U+2194  # LEFT RIGHT ARROW

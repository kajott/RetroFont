source "mc6847.pbm" 8x12

defmap "mc6847base"
    map 0 '@' 'A'..'Z' "[\]"
    map 0.30   U+2191  # UPWARDS ARROW
    map 0.31   U+2190  # LEFTWARDS ARROW
    map 1 ' '..'?'
    alias U+2B29 '*'  # BLACK SMALL DIAMOND
    alias 'a'..'z' 'A'..'Z'

defmap "mc6847t1"
    map 5 '^' 'a'..'z' "{|}~_"
    map 6 '@' 'A'..'Z' "[\]"
    map 6.30   U+2191  # UPWARDS ARROW
    map 6.31   U+2190  # LEFTWARDS ARROW
    map 7 ' '..'?'

